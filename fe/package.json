{
	"name": "organizer",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --port 8002 --host",
		"build": "vite build",
		"preview": "vite preview --port 8002 --host",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"api": "openapi-ts",
		"prepare": "cd .. && husky fe/.husky"
	},
	"devDependencies": {
		"@hey-api/openapi-ts": "^0.64.4",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.12.0",
		"@sveltejs/vite-plugin-svelte": "^4.0.0",
		"@types/wicg-file-system-access": "^2020.9.6",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"carbon-components-svelte": "^0.87.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.45.1",
		"husky": "^9.1.7",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.2.6",
		"svelte": "^5.1.5",
		"svelte-check": "^4.1.0",
		"tslib": "^2.8.1",
		"typescript": "^5.7.2",
		"vite": "^5.4.4",
		"vitest": "^2.0.5"
	},
	"type": "module",
	"dependencies": {
		"@carbon/web-components": "^2.0.1",
		"@hey-api/client-fetch": "^0.7.3",
		"@intlify/eslint-plugin-svelte": "^0.5.0",
		"@sveltejs/adapter-auto": "^3.3.1",
		"carbon-icons-svelte": "^12.3.0"
	},
	"optionalDependencies": {
		"@rollup/rollup-linux-x64-gnu": "4.34.7"
	}
}
